{
  "name": "res-rank-api",
  "version": "1.0.0",
  "description": "A GraphQL API for ResRank data.",
  "main": "src/functions/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "lint:eslint": "eslint \"src/**/*.js\"",
    "lint:prettier": "prettier \"src/**/*.js\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "fix:eslint": "eslint --fix \"src/**/*.js\"",
    "fix:prettier": "prettier --write \"src/**/*.js\"",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "serve": "firebase emulators:start --only functions,database",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions"
  },
  "keywords": [
    "res",
    "rank",
    "topic",
    "resource",
    "help",
    "learn"
  ],
  "author": "Sean Dodson",
  "license": "ISC",
  "engines": {
    "node": "12"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.1.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "apollo-server": "^2.22.2",
    "apollo-server-express": "^2.24.0",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.11.0",
    "graphql": "^15.5.0",
    "mongoose": "^5.12.5"
  }
}
